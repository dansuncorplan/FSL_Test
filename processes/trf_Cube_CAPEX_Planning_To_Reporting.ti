	ENDIF ;

	nLoopCount = nLoopCount + 1 ;

END ;

VIEWSUBSETASSIGN( sCube, sView, sDim, sSubset ) ;



## Set Source View

DATASOURCECUBEVIEW = sView ;
#endregion
#region Metadata

#****Begin: Generated Statements***
#****End: Generated Statements****
#endregion
#region Data

#****Begin: Generated Statements***
#****End: Generated Statements****



## Set Measure


IF( ATTRS( sPlanMeasureDim, vMeasure, 'rpt_Period' ) @<> '' ) ;
	sMeasure = ATTRS( sPlanMeasureDim, vMeasure, 'rpt_Period' ) ;

ELSE ;
	sMeasure = vMeasure ;

ENDIF ;



## Skip if Measure Doesn't Exist in m_rpt_Capex_Item

IF( DIMIX( sRptMeasureDim, sMeasure ) = 0 ) ;
	
	ITEMSKIP ;

ENDIF ;



## Skip if Cell is not updateable

IF( CELLISUPDATEABLE( sTargetCube, pTargetVersion, vBusiness_Unit, vBranch, vCapex_Item_List, sMeasure ) = 0 ) ;

	ITEMSKIP ;

ENDIF ;




## Enter Value to Target Version

IF( DTYPE( sRptMeasureDim, sMeasure ) @= 'S' ) ;

	CELLPUTS( vValue, sTargetCube, pTargetVersion, vBusiness_Unit, vBranch, vCapex_Item_List, sMeasure ) ;

ELSE ;

	nValue = STRINGTONUMBER( vValue ) ;

	CELLPUTN( nValue, sTargetCube, pTargetVersion, vBusiness_Unit, vBranch, vCapex_Item_List, sMeasure ) ;

ENDIF ;








#~~~~~~~~~~ProcessLogging~~~~~~~~~~#
countRecords = countRecords + 1;
#~~~~~~~~~~EndProcessLogging~~~~~~~~~~#
#endregion
#region Epilog

#****Begin: Generated Statements***
#****End: Generated Statements****



## Destroy Target View and Subsets

VIEWDESTROY( sCube, sView ) ;

SUBSETDESTROY( sPlanVersionDim, sSubset ) ;
SUBSETDESTROY( sBusinessUnitDim, sSubset ) ;
SUBSETDESTROY( sBranchDim, sSubset ) ;
SUBSETDESTROY( sCapexItemDim, sSubset ) ;
SUBSETDESTROY( sPlanMeasureDim, sSubset ) ;




#~~~~~~~~~~ProcessLogging~~~~~~~~~~#
#Variables
timeFinish=NOW;
elapsedTime=TIMST(timeFinish - timeStart,'\h:\i:\s');
dateFinish = TIMST(timeFinish,'\Y \M \d - \h:\i:\s');
status = 'Complete';
#Write status information for Finish
CELLPUTS(status,'z_Process_Logging',elementName,'Status');
CELLPUTS(dateFinish,'z_Process_Logging',elementName,'FinishTime');
CELLPUTS(elapsedTime,'z_Process_Logging',elementName,'Elapsed Time');
CELLPUTN(countRecords,'z_Process_Logging',elementName,'Records');
CELLPUTS(status,'z_Process_Logging',processName,'Status');
CELLPUTS(dateFinish,'z_Process_Logging',processName,'FinishTime');
CELLPUTS(elapsedTime,'z_Process_Logging',processName,'Elapsed Time');
#~~~~~~~~~~EndProcessLogging~~~~~~~~~~#
#endregion