## Feed Actuals to plan_Profit_and_Loss

[ 'Actual', 'Value' ] =>
DB( 'plan_Profit_and_Loss','Actual',!Currency,
!rpt_Year | ' ' | ATTRS( 'rpt_Month', !rpt_Month, 'Month_Short' ) ,
!Branch,!rpt_Business_Unit,!Account_PL,'Value' ) ;


## Feed EBITDA to rpt_Balance_Sheet

[ 'EBITDA', 'Value' ] =>
DB('rpt_Balance_Sheet',!rpt_Version,!Currency,!rpt_Month,!rpt_Year,
!Branch,!rpt_Business_Unit,'EBITDA','Value') ;



## Feed Cost of Goods Sold to rpt_Balance_Sheet

[ 'COST OF GOODS SOLD', 'Value' ] =>
DB('rpt_Balance_Sheet',!rpt_Version,!Currency,'TOTAL_RPT_MONTH','TOTAL_RPT_YEAR',
!Branch,!rpt_Business_Unit,'Inv_Cost of Goods Sold - Rolling 12 Months','Value') ;






## Total Overheads %

[ 'OVERHEADS (Excl Management Fees)' ] => [ 'Total Overheads %' ] ;



