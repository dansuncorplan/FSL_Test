SKIPCHECK ;
FEEDSTRINGS ;


## Calculations

[ 'Calculations' ] = 0 ;







## Total Overheads %

[ 'Total Overheads %' ] =
[ 'OVERHEADS (Excl Management Fees)' ] \ [ 'Total Revenue' ] ;


## EBIT %

[ 'EBIT %' ] =
[ 'EBIT' ] \ [ 'Total Revenue' ] ;


## Gross Profit %

[ 'Gross Profit %' ] =
[ 'Gross Profit' ] \ [ 'Total Revenue' ] ;


## Gross Margin %

[ 'Gross Margin %' ] =
[ 'Gross Margin' ] \ [ 'Total Revenue' ] ;










FEEDERS ;



